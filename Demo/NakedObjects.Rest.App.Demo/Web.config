    </providers>
  </entityFramework>
  <connectionStrings>
    <add name="AdventureWorksContext" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=AdventureWorks;Integrated Security=True;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
    <!--<add name="AdventureWorksContext" connectionString="Data Source=(localdb)\ProjectsV12;Initial Catalog=AdventureWorks;Integrated Security=True;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />-->
  </connectionStrings>
  
  <system.webServer>
    <modules>
      
      
      
      <remove name="Session" />
      <add name="Session" type="System.Web.SessionState.SessionStateModule" />
    <remove name="FormsAuthenticationModule" /><remove name="UrlRoutingModule-4.0" /><add name="UrlRoutingModule-4.0" type="System.Web.Routing.UrlRoutingModule" preCondition="" /></modules>
    
  <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers></system.webServer>
