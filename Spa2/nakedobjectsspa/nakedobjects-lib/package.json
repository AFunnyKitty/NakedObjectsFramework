{
    "ngPackage": {
        "$schema": "./node_modules/ng-packagr/package.schema.json",
        "lib": {
            "entryFile": "src/public_api.ts",
            "umdModuleIds": {
                "lodash": "_",
                "moment": "moment",
                "moment-timezone": "moment-timezone"
            }
        },
        "whitelistedNonPeerDependencies": [
            "auth0-js",
            "lodash",
            "lodash-es",
            "moment",
            "@beyerleinf/ngx-dnd"
        ],
        "dest": "../dist/nakedobjects-lib"
    },
    "name": "@nakedobjects/lib",
    "version": "10.0.0-beta.1",
    "description": "Single Page Application client for a Naked Objects application.",
    "license": "Apache-2.0",
    "author": "Richard Pawson <rpawson@nakedobjects.org>",
    "bugs": "https://github.com/NakedObjectsGroup/NakedObjectsFramework/issues",
    "homepage": "https://github.com/NakedObjectsGroup/NakedObjectsFramework",
    "repository": {
        "type": "git",
        "url": "git://github.com/NakedObjectsGroup/NakedObjectsFramework.git"
    },
    "angular-cli": {},
    "keywords": [
        "rest",
        "restful",
        "objects",
        "naked",
        "nakedobjects",
        "ddd",
        "domain",
        "driven",
        "design",
        "api"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "auth0-js": "^9.6.1",
        "lodash": "4.17.10",
        "lodash-es": "4.17.10",
        "moment": "2.22.2",
        "@beyerleinf/ngx-dnd": "^6.0.0-preview3"
    },
    "peerDependencies": {
        "@angular/animations": "^6.0.3",
        "@angular/common": "^6.0.3",
        "@angular/compiler": "^6.0.3",
        "@angular/core": "^6.0.3",
        "@angular/forms": "^6.0.3",
        "@angular/http": "^6.0.3",
        "@angular/platform-browser": "^6.0.3",
        "@angular/platform-browser-dynamic": "^6.0.3",
        "@angular/router": "^6.0.3",
        "core-js": "^2.5.4",
        "rxjs": "^6.0.0",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.8",
        "@angular/cli": "~6.0.8",
        "@angular/compiler-cli": "^6.0.3",
        "@angular/language-service": "^6.0.3",
        "@types/auth0-lock": "11.4.3",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/lodash": "^4.14.109",
        "@types/lodash-es": "4.17.0",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.2.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.0",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.3.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "typescript": "~2.7.2"
    }
