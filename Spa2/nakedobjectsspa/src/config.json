{
    "appPath": "http://nakedobjectsrodemo.azurewebsites.net",
    "logoffUrl": "http://nakedobjectsrodemo.azurewebsites.net/Account/Logoff",
    "urlShortCuts": [ "http://nakedobjectsrodemo.azurewebsites.net", "AdventureWorksModel" ],
    "doUrlValidation": true,
    "colors": {
        "typeMap": {
            "AdventureWorksModel.Customer": 1,
            "AdventureWorksModel.Product": 4,
            "AdventureWorksModel.Employee": 5,
            "AdventureWorksModel.SalesPerson": 6,
            "AdventureWorksModel.SpecialOffer": 7,
            "AdventureWorksModel.Vendor": 9,
            "AdventureWorksModel.WorkOrder": 12
        },
        "regexArray": [
            {
                "regex": ".*SalesOrder.*",
                "color": 2
            },
            {
                "regex": ".*SalesOrder.*",
                "color": 10
            }
        ],
        "subtypeMap": {
            "AdventureWorksModel.BusinessEntity": 8
        },
        "default": 0
    },
    "masks": {
        "currencyMasks": {
            "C": {
                "format": "decimal",
                "symbol": "GBP",
                "digits": "1.2-2"
            },
            "c": {
                "format": "decimal",
                "symbol": "GBP",
                "digits": "1.2-2"
            }
        },
        "dateMasks?": {
            "d": {
                "format": "date-time",
                "mask": "d MMM yyyy",
                "tz": "+0000"
            },
            "D": {
                "format": "date",
                "mask": "d MMM yyyy hh:mm:ss"
            },
            "T": {
                "format": "time",
                "mask": "HH:mm",
                "tz": "+0000"
            }
        }
    },
    "logLevel": "debug"
}