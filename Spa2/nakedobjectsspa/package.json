{
  "name": "nakedobjects.spa",
  "version": "10.0.0-beta.5",
  "description": "Single Page Application client for a Naked Objects application.",
  "license": "Apache-2.0",
  "author": "Richard Pawson <rpawson@nakedobjects.org>",
  "bugs": "https://github.com/NakedObjectsGroup/NakedObjectsFramework/issues",
  "homepage": "https://github.com/NakedObjectsGroup/NakedObjectsFramework",
  "repository": {
    "type": "git",
    "url": "git://github.com/NakedObjectsGroup/NakedObjectsFramework.git"
  },
  "keywords": [
    "rest",
    "restful",
    "objects",
    "naked",
    "nakedobjects",
    "ddd",
    "domain",
    "driven",
    "design",
    "api"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepackage": "node prepackage.js",
    "compile:restful-objects": "rimraf dist/nakedobjects-restful-objects && ng-packagr -p restful-objects/package.json",
    "pack:restful-objects": "cd dist/nakedobjects-restful-objects && npm pack && cd ../..",
    "install:restful-objects": "npm i dist/nakedobjects-restful-objects/nakedobjects-restful-objects-10.0.0-beta.5.tgz",
    "build:restful-objects": "yarn compile:restful-objects && yarn pack:restful-objects && yarn install:restful-objects",

    "compile:services": "rimraf dist/nakedobjects-services && ng-packagr -p services/package.json",
    "pack:services": "cd dist/nakedobjects-services && npm pack && cd ../..",
    "install:services": "npm i --no-save dist/nakedobjects-services/nakedobjects-services-10.0.0-beta.5.tgz",
    "build:services": "yarn compile:services && yarn pack:services && yarn install:services",

    "compile:view-models": "rimraf dist/nakedobjects-view-models && ng-packagr -p view-models/package.json",
    "pack:view-models": "cd dist/nakedobjects-view-models && npm pack && cd ../..",
    "install:view-models": "npm i --no-save dist/nakedobjects-view-models/nakedobjects-view-models-10.0.0-beta.5.tgz",
    "build:view-models": "yarn compile:view-models && yarn pack:view-models && yarn install:view-models",

    "compile:gemini": "rimraf dist/nakedobjects-gemini && ng-packagr -p gemini/package.json",
    "pack:gemini": "cd dist/nakedobjects-gemini && npm pack && cd ../..",
    "install:gemini": "npm i --no-save dist/nakedobjects-gemini/nakedobjects-gemini-10.0.0-beta.5.tgz",
    "build:gemini": "yarn compile:gemini && yarn pack:gemini && yarn install:gemini",

    "compile:cicero": "rimraf dist/nakedobjects-cicero && ng-packagr -p cicero/package.json",
    "pack:cicero": "cd dist/nakedobjects-cicero && npm pack && cd ../..",
    "install:cicero": "npm i --no-save dist/nakedobjects-cicero/nakedobjects-cicero-10.0.0-beta.5.tgz",
    "build:cicero": "yarn compile:cicero && yarn pack:cicero && yarn install:cicero",

    "build:clean": "rimraf dist && rimraf node_modules/@nakedobjects",
    "build:libs": "yarn build:restful-objects && yarn build:services && yarn build:view-models && yarn build:gemini && yarn build:cicero",
    "build:app": "ng build packages --no-progress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.2",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "@angular/router": "^7.1.2",
    "auth0-js": "^9.8.2",
    "core-js": "^2.6.0",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "moment": "2.22.2",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.2",
    "@angular/cli": "^7.1.2",
    "@angular/compiler-cli": "^7.1.2",
    "@angular/language-service": "^7.1.2",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.118",
    "@types/lodash-es": "4.17.1",
    "@types/node": "~10.12.12",
    "codelyzer": "~4.5.0",
    "cpr": "^3.0.1",
    "find-in-files": "^0.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.5",
    "protractor": "~5.4.1",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "3.1.6"
  }
}
