<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>RestfulObjects.Server.Nof2</id>
    <version>0.3.0-beta</version>
    <title>Restful Objects Server (for NOF2)</title>
    <authors>Naked Objects Group Ltd</authors>
    <owners>Naked Objects Group Ltd</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Add to an ASP.NET Empty Web Application project to auto-create a Restful Objects API for a domain model running under NOF2</description>
    <dependencies>
      <dependency id="RestfulObjects.Mvc" version="[7.1.0, 8)" />
      <dependency id="WebActivator" version="1.5" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" targetFramework="net40" />
      <frameworkAssembly assemblyName="System.Web.Mvc" targetFramework="net40" />
    </frameworkAssemblies>
  </metadata>
</package>