<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>NakedObjects.Spa</id>
    <version>8.0.0-rc</version>
    <authors>Naked Objects Group Ltd</authors>
    <owners>Naked Objects Group Ltd</owners>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/NakedObjectsGroup/</projectUrl>
    <iconUrl>http://www.nakedobjects.net/images/nakedobjects.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Single Page Application user interface to run against a NakedObjects.Rest server.
    </summary>
    <tags>rest restful objects naked nakedobjects ddd domain driven design api</tags>
    <description>Single Page Application client for a Naked Objects application.</description>
    <dependencies>
      <dependency id="AngularJS.Core" version="[1.5.0, 2)" />
      <dependency id="AngularJS.Route" version="[1.5.0, 2)" />
      <dependency id="AngularJS.Touch" version="[1.5.0, 2)" />
      <dependency id="angularjs.TypeScript.DefinitelyTyped" version="[4.1.9, 5)" />
      <dependency id="jQuery" version="[2.1.4, 3)" />
      <dependency id="jquery.TypeScript.DefinitelyTyped" version="[2.2.8, 3)" />
      <dependency id="jQuery.UI.Combined" version="[1.11.4, 2)" />
      <dependency id="lodash" version="[4.6.0, 5)" />
      <dependency id="lodash.TypeScript.DefinitelyTyped" version="[2.2.7, 3)" />
      <dependency id="Microsoft.TypeScript.MSBuild" version="[1.8.6, 2)" />
      <dependency id="Moment.js" version="[2.12.0, 3)" />
      <dependency id="moment.TypeScript.DefinitelyTyped" version="[1.7.6, 2)" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\Spa\NakedObjects.Spa\Content\themes\base\jquery.timepicker.css" target="Content\Content\themes\base" />
    <file src="..\..\Spa\NakedObjects.Spa\Scripts\jquery.timepicker.js" target="Content\Scripts" />
    <file src="..\..\Spa\NakedObjects.Spa\Content\partials\*.*" target="Content\Content\partials" />
    <file src="..\..\Spa\NakedObjects.Spa\Content\nakedobjects.css" target="Content\Content" />
    <file src="..\..\Spa\NakedObjects.Spa\fonts\*.*" target="Content\fonts" />
    <file src="..\..\Spa\NakedObjects.Spa\images\*.*" target="Content\images" />
    <file src="..\..\Spa\NakedObjects.Spa\Scripts\nakedobjects*.*" target="Content\Scripts" exclude="nakedobjects.config.ts; nakedobjects.services.color.config.ts" />
    <!--But config.ts.pp files will be included-->
    <file src="..\..\Spa\NakedObjects.Spa\index.html" target="Content" />
  </files>
</package>