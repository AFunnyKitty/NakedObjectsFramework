{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome against localhost, with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:49998",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
             "sourceMapPathOverrides": {
              "webpack:///E:/Users/scasc_000/Documents/GitHub/NakedObjectsFramework/Spa/nakedobjectsspa/*": "${workspaceRoot}/*"
          }
        },
        {
            "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 49998,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "sourceMapPathOverrides": {
              "webpack:///E:/Users/scasc_000/Documents/GitHub/NakedObjectsFramework/Spa/nakedobjectsspa/*": "${workspaceRoot}/*"
          }
        }
    ]
}